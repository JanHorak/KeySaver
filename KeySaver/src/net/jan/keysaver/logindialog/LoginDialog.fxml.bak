<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" fx:id="mainpane" prefHeight="146.0" prefWidth="255.0" xmlns:fx="http://javafx.com/fxml" fx:controller="net.jan.keysaver.logindialog.LoginDialogController">
  <children>
    <Label layoutX="14.0" layoutY="60.0" text="Username:" />
    <Label layoutX="14.0" layoutY="85.0" text="Password:" />
    <PasswordField fx:id="pwField" layoutX="81.0" layoutY="85.0" prefWidth="156.0" />
    <TextField layoutX="81.0" layoutY="57.0" prefWidth="156.0" />
    <Button layoutX="187.0" layoutY="111.0" mnemonicParsing="false" onAction="#login" text="Login" />
    <ImageView fx:id="imageView" fitHeight="35.0" fitWidth="153.0" layoutX="10.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
      <image>
        <Image url="@../../../../../AppData/Images/Logo_key.png" preserveRatio="false" smooth="false" />
      </image>
    </ImageView>
    <Hyperlink layoutX="15.0" layoutY="110.0" onAction="#loadRegisterPage" text="Register here" />
  </children>
</AnchorPane>
